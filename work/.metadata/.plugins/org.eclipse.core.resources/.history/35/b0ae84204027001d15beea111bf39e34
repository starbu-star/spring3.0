		CustomDAO dao = (CustomDAO)context.getBean("customDAO"); //customDAO ¶ó´Â beanÁàºÁ
		
		CustomDTO dto;
		
		//insert
		
		dto= new CustomDTO();
		dto.setId(111);
		dto.setName("¹è¼öÁö");
		dto.setAge(29);
		
		dao.insertData(dto);
		System.out.println("insert ¿Ï·á");

		
		
		
	}

}
